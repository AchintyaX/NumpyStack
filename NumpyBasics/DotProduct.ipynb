    "import numpy as np\n",
    "# declaration of numpy array\n",
    "a = np.array([1,2])\n",
    "b = np.array([2,1])\n",
    "# definition of dot product\n",
    "dot = 0\n",
    "for e, f in zip(a,b):\n",
    "    dot +=e*f\n",
    "print(dot)"
